                @Html.DisplayFor(modelItem => item.HoTen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChuyenMon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrinhDo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoDienThoai)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaKhoaNavigation.TenKhoa)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaBacSi">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaBacSi">Details</a> |
