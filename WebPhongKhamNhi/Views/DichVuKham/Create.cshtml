@model WebPhongKhamNhi.Models.Dichvukham

@{
    ViewData["Title"] = "Create";
}

<h1>Thêm mới dịch vụ khám</h1>

<hr />
@if (ViewData["DichVuDuplicateName"] != null)
{
    <div class="alert alert-danger">
        <p>@ViewData["DichVuDuplicateName"]</p>
    </div>
}

<div class="row">
    <div class="col-md-4">

        <form asp-action="Create">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TenDichVu" class="control-label"></label>
                <input asp-for="TenDichVu" class="form-control" />
                <span asp-validation-for="TenDichVu" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ChiPhi" class="control-label"></label>
                <input asp-for="ChiPhi" class="form-control" />
                <span asp-validation-for="ChiPhi" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Thêm" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Quay lại</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
